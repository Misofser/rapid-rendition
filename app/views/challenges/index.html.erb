<h1>Challenges</h1>

  <div class="challenge-list">
    <ul>
      <% @challenges.each do |challenge| %>
        <li>
          <a href="<%= challenge_path(challenge) %>">
            <%= challenge.title %>
          </a>
        </li>
      <% end %>
    </ul>
  </div>

  <% if current_user && current_user.admin? %> 
    <p> 
      <%= link_to "New Challenge", new_challenge_path %>
    </p> 
  <% end %> 
